{
  "includes": [],
  "sinks": [{
    "name": "stderr_color",
    "type": "stderr_color_sink",
    "args": {
      "level_colors": [{
        "level": "debug",
        "color": "green"
      },{
        "level": "info",
        "color": "reset"
      },{
        "level": "warn",
        "color": "yellow"
      },{
        "level": "error",
        "color": "red"
      }
      ]
    }
  },{
    "name": "daily_file",
    "type": "daily_file_sink",
    "args": {
      "base_filename": "./logs/daily.log",
      "rotation_hour": 0,
      "rotation_minute": 0,
      "truncate": false,
      "max_files": 0
    }
  },{
    "name": "rotate_file",
    "type": "rotating_file_sink",
    "args": {
      "base_filename": "./logs/rotate.log",
      "max_size": 10,
      "max_file": 10
    }
  }
  ],
  "loggers": [{
