
if(NOT TARGET bn256::bn256)
    add_library(bn256::bn256 STATIC IMPORTED)

    set_target_properties(bn256::bn256 PROPERTIES
        INTERFACE_INCLUDE_DIRECTORIES "${_VCPKG_INSTALLED_DIR}/${VCPKG_TARGET_TRIPLET}/include"
        IMPORTED_LINK_INTERFACE_LANGUAGES "C"
    )


    find_library(VCPKG_BN256_LIBRARY_RELEASE NAMES libbn256 bn256 PATHS "${_VCPKG_INSTALLED_DIR}/${VCPKG_TARGET_TRIPLET}/lib" NO_DEFAULT_PATH)
    if(EXISTS "${VCPKG_BN256_LIBRARY_RELEASE}")
        set_property(TARGET bn256::bn256 APPEND PROPERTY IMPORTED_CONFIGURATIONS "Release")
        set_target_properties(
          bn256::bn256
          PROPERTIES
          IMPORTED_LINK_INTERFACE_LANGUAGES_RELEASE "CXX"
          IMPORTED_LOCATION_RELEASE "${VCPKG_BN256_LIBRARY_RELEASE}"
        )
    endif()

    find_library(VCPKG_BN256_LIBRARY_DEBUG NAMES libbn256 bn256 PATHS "${_VCPKG_INSTALLED_DIR}/${VCPKG_TARGET_TRIPLET}/debug/lib" NO_DEFAULT_PATH)
    if(EXISTS "${VCPKG_BN256_LIBRARY_DEBUG}")
        set_property(TARGET bn256::bn256 APPEND PROPERTY IMPORTED_CONFIGURATIONS "Debug")
        set_target_properties(
          bn256::bn256
          PROPERTIES
          IMPORTED_LINK_INTERFACE_LANGUAGES_DEBUG "CXX"
          IMPORTED_LOCATION_DEBUG "${VCPKG_BN256_LIBRARY_DEBUG}"
        )
    endif()
endif()